<table id='moreInfoTable' class="table striped-table">
	<thead class="infotable-head">
	<tr><th>Site</th><th class="center">Patient</th><th class="center">Biosamples</th>
	{{#each biosampleFields}}
		<th class="center"><span>{{label}}</span></th>
	{{/each}}
	</tr>
	</thead>
	
	<tbody>
	{{#each resources}}
	<tr><td><span title="{{description}}">{{name}}</span></td>
		<td class="center"><span id="{{uuid}}-patients"> </span></td>
		<td class="center"><span id="{{uuid}}-biosamples"> </span></td>
		{{#each ../biosampleFields}}
			<td class="center" id="{{../uuid}}-{{id}}"><div>  </div></td>
		{{/each}}
	</tr>
	{{/each}}
	
	</tbody>
</table>
